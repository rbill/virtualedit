<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated VObjectModel -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="VObjectModel.presentation.VObjectModelModelWizardID"
            name="%_UI_VObjectModelModelWizard_label"
