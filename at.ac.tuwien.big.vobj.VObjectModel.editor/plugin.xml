   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated VObjectModel -->
      <editor
            id="VObjectModel.presentation.VObjectModelEditorID"
            name="%_UI_VObjectModelEditor_label"
            icon="icons/full/obj16/VObjectModelModelFile.gif"
            extensions="vobjectmodel"
            class="VObjectModel.presentation.VObjectModelEditor"
            contributorClass="VObjectModel.presentation.VObjectModelActionBarContributor">
